{
  "name": "novu",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "email:build": "email build",
    "email:dev": "email dev --dir ./src/features/emails --port 3001",
    "email:export": "email export",
    "stripe:listen": "stripe listen --forward-to=localhost:3000/api/webhooks --project-name={{STRIPE_CLI_PROJECT_NAME}}",
    "generate-types": "npx supabase gen types typescript --project-id {{SUPABASE_PROJECT_ID}} --schema public > src/libs/supabase/types.ts",
    "supabase:link": "env-cmd -f ./.env.local supabase link --project-ref {{SUPABASE_PROJECT_ID}}",
    "generator:transform": "node ./generator/transform.js --manifest ./generator/example-manifests/saas.json",
    "preview:container": "docker build -t ai-builder-preview . && docker run --rm -p 3000:3000 --env-file .env.local ai-builder-preview",
    "ci:checks": "sh ./ci/checks.sh",
    "ci:export-check": "sh ./ci/export-check.sh",
    "migration:new": "supabase migration new",
    "migration:up": "supabase migration up --linked --debug && npm run generate-types",
    "migration:squash": "supabase migration squash --linked"
  },
  "dependencies": {
    "@google/genai": "^0.11.0",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@react-email/components": "^0.5.0",
    "@stackframe/stack": "^2.8.28",
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.47.12",
    "@vercel/analytics": "^1.4.1",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.4",
    "fast-glob": "^3.3.2",
    "lucide-react": "^0.474.0",
    "marked": "^16.1.2",
    "next": "^15.1.4",
    "next-route-handler-pipe": "^1.0.5",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-email": "^4.2.8",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.4.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "resend": "^4.1.1",
    "shiki": "^3.9.2",
    "stripe": "^14.25.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "unique-names-generator": "^4.7.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/react": "19.0.4",
    "@types/react-dom": "19.0.2",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.1",
    "drizzle-kit": "^0.31.4",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.1.4",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "postcss": "^8.4.49",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "supabase": "^1.226.4",
    "typescript": "^5.7.3"
  },
  "overrides": {
    "@types/react": "19.0.4",
    "@types/react-dom": "19.0.2"
  }
}
